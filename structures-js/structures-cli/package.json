{
    "name": "@kinotic/structures-cli",
    "version": "3.5.0-beta.7",
    "description": "Structures CLI provides the ability to interact with the Structures Server",
    "author": "Kinotic Developers",
    "bin": {
        "structures": "./bin/run.js"
    },
    "homepage": "https://kinotic-foundation.github.io/structures/",
    "repository": "https://github.com/Kinotic-Foundation/structures",
    "license": "MIT",
    "main": "dist/index.js",
    "type": "module",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@inquirer/prompts": "^8.0.2",
        "@kinotic/continuum-client": "2.14.5",
        "@kinotic/continuum-idl": "^2.0.4",
        "@kinotic/structures-api": "^3.5.0-beta.16",
        "@oclif/core": "^4.8.0",
        "@oclif/plugin-autocomplete": "^3.2.39",
        "@oclif/plugin-help": "^6",
        "@oclif/plugin-not-found": "^3.2.73",
        "@oclif/plugin-plugins": "^5.4.54",
        "@oclif/plugin-update": "^4.7.16",
        "@oclif/plugin-warn-if-update-available": "^3.1.53",
        "c12": "^3.3.2",
        "chalk": "^5.6.2",
        "glob": "^13.0.0",
        "graphql": "^16.12.0",
        "liquidjs": "^10.24.0",
        "open": "^11.0.0",
        "p-timeout": "^7.0.1",
        "radash": "^12.1.1",
        "reflect-metadata": "^0.2.2",
        "terminal-link": "^5.0.0",
        "ts-morph": "^27.0.2",
        "uuid": "^13.0.0",
        "ws": "^8.18.3",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "@oclif/test": "^4.1.15",
        "@types/chai": "^5",
        "@types/inquirer": "^9.0.9",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.10.1",
        "@types/uuid": "^11.0.0",
        "@types/ws": "^8.18.1",
        "chai": "^6",
        "eslint": "^9",
        "eslint-config-oclif": "^6",
        "eslint-config-prettier": "^10",
        "mocha": "^11",
        "oclif": "^4.22.52",
        "shx": "^0.4.0",
        "tslib": "^2.8.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "oclif": {
        "bin": "structures",
        "dirname": "structures",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins",
            "@oclif/plugin-update",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-autocomplete"
        ],
        "topicSeparator": " ",
        "topics": {}
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b && pnpm run copy-files",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "pnpm lint",
        "prepack": "pnpm build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "copy-files": "cp -r ./src/templates/  ./dist/templates/"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/https://github.com/Kinotic-Foundation/structures/structures/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
